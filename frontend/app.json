{
  "expo": {
    "name": "CampusBuddy",
    "slug": "frontend",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./src/assets/CampusBuddyIcon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/CampusBuddyIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3a86ff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false,
        "googleMapsApiKey": "AIzaSyDNqXs5VfojrTbjYKVOilqaYlkfMW1BnJE"
      },
      "bundleIdentifier": "com.example.campusbuddy",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.example.campusbuddy",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyATVa14cULE6unb0p7DUj9MW4v6tzRWzdE"
        }
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6fdddfb2-ae12-471d-ac8a-feaff7a00ee5"
      }
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.example.campusbuddy",
          "enableGooglePay": true
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-localization"
    ],
    "owner": "campusbuddy"
  }
}
